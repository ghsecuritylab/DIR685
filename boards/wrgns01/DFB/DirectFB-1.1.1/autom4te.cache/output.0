  case "$host_cpu" in
  x86_64*|s390x*|powerpc64*)
    echo '#line __oline__ "configure"' > conftest.$ac_ext
    if { (eval echo "$as_me:$LINENO: \"$ac_compile\"") >&5
  (eval $ac_compile) 2>&5
  ac_status=$?
  echo "$as_me:$LINENO: \$? = $ac_status" >&5
  (exit $ac_status); }; then
      case `/usr/bin/file conftest.$ac_objext` in
      *64-bit*)
